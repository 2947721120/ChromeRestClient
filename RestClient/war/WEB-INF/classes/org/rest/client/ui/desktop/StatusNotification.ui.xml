<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel stylePrimaryName="Status_Notification_flashMessage" ui:field="mainPanel">
		<g:HTMLPanel stylePrimaryName="Status_Notification_wrapper">
			<div class="Status_Notification_info">
				<g:InlineLabel styleName="Status_Notification_textHiddable" ui:field="message" text="(empty)"></g:InlineLabel>
			</div>
			<g:HTMLPanel ui:field="actions" stylePrimaryName="Status_Notification_dismiss">
				<g:Anchor ui:field="flashDismiss" href="about:blank" styleName="Status_Notification_dismissAnchor" title="Dismiss" text="dismiss"></g:Anchor>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>