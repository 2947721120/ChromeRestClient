<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" >
	<ui:with field="appResources" type="org.rest.client.resources.AppResources" />
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel ui:field="container" addStyleNames="{appResources.appCss.historyWrapper}">
		<div class="{appResources.appCss.historyListRow} {appResources.appCss.flex}">
			<g:InlineLabel styleName="{appResources.appCss.historyDate} {appResources.appCss.flex}" ui:field="dateLabel"></g:InlineLabel>
			<g:InlineLabel styleName="{appResources.appCss.historyMethod} {appResources.appCss.flex}" ui:field="methodLabel"></g:InlineLabel>
			<g:HTMLPanel styleName="{appResources.appCss.historyUrl} {appResources.appCss.flex}" ui:field="urlLabel">
				<g:InlineLabel styleName="{appResources.appCss.historyUrlValue}" ui:field="urlValue"></g:InlineLabel>
			</g:HTMLPanel>
			<span class="{appResources.appCss.historyAction} {appResources.appCss.flex} {appResources.appCss.inlineButtonsGroup}">
				<g:Button ui:field="select" styleName="{appResources.appCss.inlineButton} {appResources.appCss.historySelectButton}">Select</g:Button>
				<g:Button ui:field="delete" styleName="{appResources.appCss.inlineButton} {appResources.appCss.historySelectButton}">Delete</g:Button>
			</span>
		</div>
		<div class="{appResources.appCss.hidden} {appResources.appCss.flex} {appResources.appCss.historyDetailed}" ui:field="detailedPanel">
			<span class="{appResources.appCss.historyEncoding} {appResources.appCss.flex} " ui:field="encoding"></span>
			<span class="{appResources.appCss.historyPayload} {appResources.appCss.flex} {appResources.appCss.historyFlex1}" ui:field="payload"></span>
			<span class="{appResources.appCss.historyHeaders} {appResources.appCss.flex} {appResources.appCss.historyFlex1}" ui:field="headers"></span>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>