<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:rcui="urn:import:org.rest.client.ui.html5">
	<ui:with field="appResources" type="org.rest.client.resources.AppResources"/>
	<ui:style type="org.rest.client.ui.desktop.MenuItemViewImpl.WidgetStyle">
		.item{
			list-style: none;
			padding: 5px 0;
			margin: 4px 0;
			cursor: pointer;
			padding-left: 10px;
			border-left: 3px white solid;

		}
		.selected {
			color: #DD4B39;
			border-left: 3px #DD4B39 solid;
		}
		.selected > ul {
			color: initial;
			border-left: 3px white solid;
		}
		.hover{
			background-color: #EEE;
		}
	</ui:style>
	<rcui:ListItem ui:field="mainListItem" styleName="{style.item}">
	
	</rcui:ListItem>
</ui:UiBinder>