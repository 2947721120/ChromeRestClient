<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:DialogBox ui:field="dialog" modal="true"
		animationEnabled="true" glassEnabled="true">
		<g:HTMLPanel ui:field="contents" styleName="container">
			<div class="dialogTitle">
				<span>Edit project</span>
			</div>
			<div>
				<g:TextBox ui:field="projectNameInput" width="400px"></g:TextBox>
			</div>
			<div ui:field="deleteContainer" class="deleteProjectContainer">
				<img src="img/5_content_discard.png" class="deleteProjectImage" title="Delete project"/>
				<g:Anchor ui:field="deleteProjectAnchor" href="about:blank">Delete project and associated requests</g:Anchor>
			</div>
			<div class="hidden Add_Encoding_View_errorField" ui:field="errorField"></div>
			<div class="dialogButtons">
				<g:Button styleName="button" ui:field="save">Save</g:Button>
				<g:Button styleName="button" ui:field="cancel">Cancel</g:Button>
			</div>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder>