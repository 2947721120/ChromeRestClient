<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.row{}
		.block{}
	</ui:style>
	<g:HTMLPanel styleName="{style.row}">
		<g:SimplePanel ui:field="keyPanel" styleName="{style.block}"></g:SimplePanel>
		<g:SimplePanel ui:field="valuePanel" styleName="{style.block}"></g:SimplePanel>
		<g:HTMLPanel ui:field="actionsPanel" styleName="{style.block}"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 