<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h5="urn:import:org.rest.client.ui.html5">
	<ui:with field="appResources" type="org.rest.client.resources.AppResources"/>
	<ui:style type="org.rest.client.ui.desktop.widget.RequestHeadersWidget.HeaderStyle">
		.rawInput{
			box-sizing: border-box;
			width: 100%;
			height: 100px;
			border:none;
		}
		.hasSupport{
			
		}
		.headerSupportHint{
			background-image: url(data:image/png;base64,);
			background-repeat: no-repeat;
			opacity: 0.2;
			width: 16px;
			height: 16px;
			position: absolute;
			right: 3px;
			top: 7px;
			cursor: pointer;
			display: none;
		}
		.keyBoxContainer{
			position: relative;
		}
		
		.headerDesc{}
		.headerExample{}
		.headersDescPopup{}
		.headersDescPopup .headerExample {
			font-style: italic;
			font-size: 11px;
		}
		.headersDescPopup .headerExample:before {
			content: "eg. ";
			font-weight: bold;
		}
		.keyBoxContainer.hasSupport .headerSupportHint{
			display: block;
		}
	</ui:style> 
	<g:HTMLPanel styleName="{appResources.appCss.tabsPanel}">
		
		<div class="{appResources.appCss.tabs}">
			<div class="{appResources.appCss.inlineButtonsGroup}">
				<g:InlineLabel ui:field="rawTab" styleName="{appResources.appCss.inlineButton} {appResources.appCss.inlineButtonChecked}">Raw</g:InlineLabel>
				<g:InlineLabel ui:field="formTab" styleName="{appResources.appCss.inlineButton}">Form</g:InlineLabel>
			</div>
			<span class="{appResources.appCss.tabCaption}">Headers</span>
		</div>
		<div ui:field="tabContent" class="{appResources.appCss.tabsContent}">
			<section data-tab="raw" class="{appResources.appCss.tabContent} {appResources.appCss.tabContentCurrent}">
				<g:TextArea ui:field="headersRawInput" styleName="{style.rawInput}"></g:TextArea>
			</section>
			<section data-tab="form" class="{appResources.appCss.tabContent}">
				
				<!-- Add new form value button -->
				<g:Anchor styleName="{appResources.appCss.addValueAnchor}" ui:field="addHeader" href="#">Add new header</g:Anchor>
					
				<g:HTMLPanel ui:field="headersFormPanel">
					
				</g:HTMLPanel>
			</section>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>