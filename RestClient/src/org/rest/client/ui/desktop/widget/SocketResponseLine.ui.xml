<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.rest.client.ui.desktop.widget.SocketResponseLine.WidgetStyle">
		.row{
			display: -webkit-box;
			-webkit-box-align: start;
			margin: 4px 0;
			padding: 4px 0;
			border-bottom: 1px #E5E5E5 solid;
		}
		.row:last-child{border-bottom: none;}
		.type{
			display: -webkit-box;
			font-size: x-small;
			margin-right: 10px;
			margin-left: 7px;
		}
		.message{white-space: pre-wrap;}
		.received{}
		.received .type{}
	</ui:style>
	<g:HTMLPanel ui:field="container" styleName="{style.row}">
		<span ui:field="typeDisplay" class="{style.type}"></span>
		<span ui:field="messageDisplay" class="{style.message}"></span>
	</g:HTMLPanel>
</ui:UiBinder> 