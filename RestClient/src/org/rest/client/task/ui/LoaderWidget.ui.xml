<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel addStyleNames="Loader_loader-wrapper">
		<div class="Loader_loader-info">
			<g:Label ui:field="loadingInfo" addStyleNames="loadingInfo">Loading...</g:Label>
		</div>
		<div class="loader-container">
			<div class="Loader_holder">
				<div class="Loader_loaded" ui:field="progress"></div>
				<div class="Loader_pacman loaderImage" ui:field="pacman"></div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 