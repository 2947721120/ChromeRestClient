<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:p1="urn:import:com.kalicinscy.web.restclient.client.api">
	<g:HTMLPanel>
		<div class="date-time-fill-helper">
			<div class="date-picker-wrapper">
				<div class="date-picker">
					<d:DatePicker ui:field="picker" />
				</div>
			</div>
			<div class="time-selector-wrapper">
				<div class="time-selector">
					<span class="set-time-legend">Set time</span>
					<div class="time-fields">
						<p1:HTML5InputNumber text="23" visibleLength="3"
							required="true" maxLength="2" ui:field="hrsValue" alignment="RIGHT"
							autocomplete="true" autofocus="true" />
						<span class="time-separator">:</span>
						<p1:HTML5InputNumber text="23" visibleLength="3"
							required="true" maxLength="2" ui:field="minValue" autocomplete="true"
							textAlignment="ALIGN_RIGHT" />

						<span class="time-separator">:</span>
						<p1:HTML5InputNumber text="23" visibleLength="3"
							required="true" maxLength="2" ui:field="secValue" autocomplete="true"
							textAlignment="ALIGN_RIGHT" />
					</div>
					<div class="set-current-button">
						<g:Button ui:field="setCurrent">Set current</g:Button>
					</div>
				</div>
			</div>
		</div>
		<div class="date-picker-buttons">
			<div class="buttons-right">
				<g:Button ui:field="okButton">OK</g:Button>
				<g:Button ui:field="cancelButton">Cancel</g:Button>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>