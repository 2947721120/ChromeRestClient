<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style type="com.restclient.client.widgets.JSONViewer.ParserStyle">
		@def indent 15px; 
		.prettyPrint{}
		.bodyPanel {
			font-family: monospace;
			font-size: 10pt;
			color: black;
			cursor: text;
		}
		.numeric{
			color: blue;
		}
		.nullValue{
			color: #708;
		}
		.booleanValue{
			color: #708;
		}
		.punctuation{
			color: black;
		}
		.stringValue{
			color: #48A;
		}
		.node{}
		.arrayCounter{
			color: gray;
			font-size: 11px;
		}
		.arrayCounter:before {
			content: "size";
		}
		.keyName{
			color: rgb(206,123,0)
		}
	</ui:style>
	<g:HTML ui:field="result" styleName="bodyPanel"></g:HTML>
</ui:UiBinder>