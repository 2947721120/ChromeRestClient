<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="../common.css">
		.mainPanel{
	    	
	    }
	    .link{
			font-size: 12px;
			max-width: 1024px;
			width: 100%;
		}
	</ui:style>
	<g:DecoratedPopupPanel ui:field="dialog" animationEnabled="true" autoHideEnabled="true" modal="true">
		<g:HTMLPanel ui:field="contents" styleName="{style.mainPanel}">
			<div ui:field="loader">
				loading...
			</div>
			<div ui:field="data" style="display:none;">
				<p>Share this key with coworkers. Paste it in "saved requests" panel.</p>
				<g:TextBox readOnly="true" addStyleNames="{style.link}" ui:field="link"></g:TextBox>
			</div>
		</g:HTMLPanel>
	</g:DecoratedPopupPanel>
</ui:UiBinder>