<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.pager{
			display: -webkit-box;
			-webkit-box-pack: center;
			-webkit-box-align: center;
			-webkit-box-orient: horizontal;
			display: -moz-box;
			-moz-box-pack: center;
			-moz-box-align: center;
			-moz-box-orient: horizontal;
		}
		.mainTable{table-layout: fixed;}
<!-- 		.saveControls{ -->
<!-- 			padding: 10px; -->
<!-- 		} -->
	</ui:style>
	<g:HTMLPanel ui:field="tableContainer">
<!-- 		<div class="{style.saveControls}"> -->
<!-- 			<g:Button ui:field="exportButton" text="export list"></g:Button> -->
<!-- 			<g:Button ui:field="importButton" text="import list"></g:Button> -->
<!-- 		</div> -->
		<c:CellTable addStyleNames="{style.mainTable}" width="100%" ui:field="table" pageSize="12"></c:CellTable>
		<div class="{style.pager}">
			<c:SimplePager ui:field="pager"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 