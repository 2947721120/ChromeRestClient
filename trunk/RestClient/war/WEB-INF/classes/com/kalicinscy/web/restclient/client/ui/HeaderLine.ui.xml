<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
<g:HTMLPanel>
<div class="hint-wrapper">
	<g:Anchor ui:field="title"/>
	
	<div class="hint-info" ui:field="hintParent">
		<span class="help-desc-item">
			<span class="hd-name" ui:field="headerName"></span><br/>
			<span class="hd-desc" ui:field="desc"></span>
			<span class="hd-example hidden" ui:field="example"></span>
		</span>
	</div>
</div>
</g:HTMLPanel>
</ui:UiBinder>